generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Products {
  id        Int     @id @default(autoincrement())
  title     String
  details   String
  available Boolean @default(true)
}

model new_User {
  id       Int     @id @default(autoincrement())
  email    String
  userName String
  password String
  admin    Boolean @default(false)
}
